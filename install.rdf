<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>ScriptOff@8pecxstudios.com</em:id>
    <em:version>1.0.3</em:version>
        <em:type>2</em:type>
	<em:iconURL>chrome://scriptoff/skin/icon32.png</em:iconURL>
	<em:icon64URL>chrome:/scriptoff/skin/icon64.png</em:icon64URL>	
    <em:targetApplication>
      <Description>
		<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>29.*</em:minVersion>
                <em:maxVersion>38.0</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:name>ScriptOff</em:name>
        <em:description>Disabling JavaScript per tab for development testing just got easier, This addon will add a entry to the browsers right click context menu to toggle the JavaScript in that tab also a button to the browsers customize pallet.</em:description>
		<em:creator>8pecxstudios</em:creator>
    <em:homepageURL>https://8pecxstudios.com/</em:homepageURL>
	<!-- Special Thanks List -->
	<em:translator>Egorro (ru (Russian))</em:translator>	
  </Description>      
